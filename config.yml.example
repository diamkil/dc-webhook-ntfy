topics:
  topic1:
    filters:
      - key: "event"
        value: "error"
        format: "Error occurred: {{error}}"
      - key: "event"
        value: "warning"
        discard: true
    format: "{{raw_message}}"

  topic2:
    filters:
      - key: "priority"
        value: "high"
        format: "High priority alert: {{message}} from {{sender}}"
    format: "{{raw_message}}"

  topic3:
    filters:
      - key: "priority"
        value: "high"
        format: "High priority alert: {{message}} from {{sender}}"
    format: "{{raw_message}}"
